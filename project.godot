; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Wing Commander Saga"
run/main_scene="res://scenes/main/bootstrap.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Scenes="*res://addons/scene_manager/scenes.gd"
Signal_Debugger="*res://addons/SignalVisualizer/Debugger/SignalDebugger.gd"
GameStateManager="*res://autoload/game_state_manager.gd"
PhysicsManager="*res://autoload/physics_manager.gd"
InputManager="*res://autoload/input_manager.gd"
ConfigurationManager="*res://autoload/configuration_manager.gd"
SaveGameManager="*res://autoload/SaveGameManager.gd"
ObjectManager="*res://autoload/object_manager.gd"
SceneManager="*res://addons/scene_manager/SceneManager.tscn"
VPResourceManager="*res://autoload/vp_resource_manager.gd"
WCSAssetLoader="*res://addons/wcs_asset_core/loaders/asset_loader.gd"
WCSAssetRegistry="*res://addons/wcs_asset_core/loaders/registry_manager.gd"
WCSAssetValidator="*res://addons/wcs_asset_core/loaders/validation_manager.gd"

[display]

window/size/viewport_width=1024
window/size/viewport_height=768
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/SignalVisualizer/plugin.cfg", "res://addons/gdUnit4/plugin.cfg", "res://addons/gfred2/plugin.cfg", "res://addons/godot_mcp/plugin.cfg", "res://addons/scene_manager/plugin.cfg", "res://addons/wcs_asset_core/plugin.cfg")

[input]

debug_toggle={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":4194332,"key_label":0,"unicode":0,"location":0,"echo":false,"script":null)
]
}

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
